N
Nvoid vListInitialise( xList *pxList )
N{
N	/* The list structure contains a list item which is used to mark the
N	end of the list.  To initialise the list the list end is inserted
N	as the only list entry. */
N	pxList->pxIndex = ( xListItem * ) &( pxList->xListEnd );
N
N	/* The list end value is the highest possible value in the list to
N	ensure it remains at the end of the list. */
N	pxList->xListEnd.xItemValue = portMAX_DELAY;
X	pxList->xListEnd.xItemValue = ( portTickType ) 0xffffffff;
N
N	/* The list end next and previous pointers point to itself so we know
N	when the list is empty. */
N	pxList->xListEnd.pxNext = ( xListItem * ) &( pxList->xListEnd );
N	pxList->xListEnd.pxPrevious = ( xListItem * ) &( pxList->xListEnd );
N
N	pxList->uxNumberOfItems = ( unsigned portBASE_TYPE ) 0U;
X	pxList->uxNumberOfItems = ( unsigned long ) 0U;
N}
N/*-----------------------------------------------------------*/
N
Nvoid vListInitialiseItem( xListItem *pxItem )
N{
N	/* Make sure the list item is not recorded as being on a list. */
N	pxItem->pvContainer = NULL;
X	pxItem->pvContainer = 0;
N}
N/*-----------------------------------------------------------*/
N
Nvoid vListInsertEnd( xList *pxList, xListItem *pxNewListItem )
N{
Nvolatile xListItem * pxIndex;
N
N	/* Insert a new list item into pxList, but rather than sort the list,
N	makes the new list item the last item to be removed by a call to
N	pvListGetOwnerOfNextEntry.  This means it has to be the item pointed to by
N	the pxIndex member. */
N	pxIndex = pxList->pxIndex;
N
N	pxNewListItem->pxNext = pxIndex->pxNext;
N	pxNewListItem->pxPrevious = pxList->pxIndex;
N	pxIndex->pxNext->pxPrevious = ( volatile xListItem * ) pxNewListItem;
N	pxIndex->pxNext = ( volatile xListItem * ) pxNewListItem;
N	pxList->pxIndex = ( volatile xListItem * ) pxNewListItem;
N
N	/* Remember which list the item is in. */
N	pxNewListItem->pvContainer = ( void * ) pxList;
N
N	( pxList->uxNumberOfItems )++;
N}
N/*-----------------------------------------------------------*/
N
Nvoid vListInsert( xList *pxList, xListItem *pxNewListItem )
N{
Nvolatile xListItem *pxIterator;
NportTickType xValueOfInsertion;
N
N	/* Insert the new list item into the list, sorted in ulListItem order. */
N	xValueOfInsertion = pxNewListItem->xItemValue;
N
N	/* If the list already contains a list item with the same item value then
N	the new list item should be placed after it.  This ensures that TCB's which
N	are stored in ready lists (all of which have the same ulListItem value)
N	get an equal share of the CPU.  However, if the xItemValue is the same as
N	the back marker the iteration loop below will not end.  This means we need
N	to guard against this by checking the value first and modifying the
N	algorithm slightly if necessary. */
N	if( xValueOfInsertion == portMAX_DELAY )
X	if( xValueOfInsertion == ( portTickType ) 0xffffffff )
N	{
N		pxIterator = pxList->xListEnd.pxPrevious;
N	}
N	else
N	{
N		/* *** NOTE ***********************************************************
N		If you find your application is crashing here then likely causes are:
N			1) Stack overflow -
N			   see http://www.freertos.org/Stacks-and-stack-overflow-checking.html
N			2) Incorrect interrupt priority assignment, especially on Cortex-M3
N			   parts where numerically high priority values denote low actual
N			   interrupt priories, which can seem counter intuitive.  See
N			   configMAX_SYSCALL_INTERRUPT_PRIORITY on http://www.freertos.org/a00110.html
N			3) Calling an API function from within a critical section or when
N			   the scheduler is suspended.
N			4) Using a queue or semaphore before it has been initialised or
N			   before the scheduler has been started (are interrupts firing
N			   before vTaskStartScheduler() has been called?).
N		See http://www.freertos.org/FAQHelp.html for more tips.
N		**********************************************************************/
N
N		for( pxIterator = ( xListItem * ) &( pxList->xListEnd ); pxIterator->pxNext->xItemValue <= xValueOfInsertion; pxIterator = pxIterator->pxNext )
N		{
N			/* There is nothing to do here, we are just iterating to the
N			wanted insertion position. */
N		}
N	}
N
N	pxNewListItem->pxNext = pxIterator->pxNext;
N	pxNewListItem->pxNext->pxPrevious = ( volatile xListItem * ) pxNewListItem;
N	pxNewListItem->pxPrevious = pxIterator;
N	pxIterator->pxNext = ( volatile xListItem * ) pxNewListItem;
N
N	/* Remember which list the item is in.  This allows fast removal of the
N	item later. */
N	pxNewListItem->pvContainer = ( void * ) pxList;
N
N	( pxList->uxNumberOfItems )++;
N}
N/*-----------------------------------------------------------*/
N
Nunsigned portBASE_TYPE uxListRemove( xListItem *pxItemToRemove )
Xunsigned long uxListRemove( xListItem *pxItemToRemove )
N{
NxList * pxList;
N
N	pxItemToRemove->pxNext->pxPrevious = pxItemToRemove->pxPrevious;
N	pxItemToRemove->pxPrevious->pxNext = pxItemToRemove->pxNext;
N
N	/* The list item knows which list it is in.  Obtain the list from the list
N	item. */
N	pxList = ( xList * ) pxItemToRemove->pvContainer;
N
N	/* Make sure the index is left pointing to a valid item. */
N	if( pxList->pxIndex == pxItemToRemove )
N	{
N		pxList->pxIndex = pxItemToRemove->pxPrevious;
N	}
N
N	pxItemToRemove->pvContainer = NULL;
X	pxItemToRemove->pvContainer = 0;
N	( pxList->uxNumberOfItems )--;
N
N	return pxList->uxNumberOfItems;
N}
